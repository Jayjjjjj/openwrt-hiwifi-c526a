@@ -0,0 +1,13 @@
+#!/bin/sh /etc/rc.common
+
+STOP=60
+
+stop() {
+	. /lib/functions.sh
+
+	case $(board_name) in
+		hiwifi,c526a)
+			rmmod mt7615e
+			;;
+	esac
+}
